<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
    <servlet>
        <servlet-name>fetchGerritStatsServlet</servlet-name>
        <servlet-class>com.appdynamics.tools.gerritstats.FetchGerritStatsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>fetchGerritStatsServlet</servlet-name>
        <url-pattern>*.fetchGerritStatsServlet</url-pattern>
    </servlet-mapping>


</web-app>